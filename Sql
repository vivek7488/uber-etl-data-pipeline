CREATE OR REPLACE TABLE `data-with-vivek.uber_data_engineering_yt.tbl_analytics` AS
WITH base_fact AS (
  SELECT *
  FROM `data-with-vivek.uber_data_engineering_yt.fact_table`
)
SELECT
  f.trip_id,
  f.VendorID,
  d.tpep_pickup_datetime,
  d.tpep_dropoff_datetime,
  p.passenger_count,
  t.trip_distance,
  r.rate_code_name,
  pl.pickup_latitude,
  pl.pickup_longitude,
  dl.dropoff_latitude,
  dl.dropoff_longitude,
  pay.payment_type_name,
  f.fare_amount,
  f.extra,
  f.mta_tax,
  f.tip_amount,
  f.tolls_amount,
  f.improvement_surcharge,
  f.total_amount
FROM base_fact f
JOIN `data-with-vivek.uber_data_engineering_yt.datetime_dim` d
  ON f.datetime_id = d.datetime_id
JOIN `data-with-vivek.uber_data_engineering_yt.passenger_count_dim` p
  ON f.passenger_count_id = p.passenger_count_id
JOIN `data-with-vivek.uber_data_engineering_yt.trip_distance_dim` t
  ON f.trip_distance_id = t.trip_distance_id
JOIN `data-with-vivek.uber_data_engineering_yt.rate_code_dim` r
  ON f.rate_code_id = r.rate_code_id
JOIN `data-with-vivek.uber_data_engineering_yt.pickup_location_dim` pl
  ON f.pickup_location_id = pl.pickup_location_id
JOIN `data-with-vivek.uber_data_engineering_yt.dropoff_location_dim` dl
  ON f.dropoff_location_id = dl.dropoff_location_id
JOIN `data-with-vivek.uber_data_engineering_yt.payment_type_dim` pay
  ON f.payment_type_id = pay.payment_type_id;
